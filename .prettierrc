{
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^@api/(.*)$",
    "^@pages/(.*)$",
    "^@routes/(.*)$",
    "^@components/(.*)$",
    "^@hooks/(.*)$",
    "^@styles/(.*)$",
    "^@stores/(.*)$",
    "^@contexts/(.*)$",
    "^@type/(.*)$",
    "^@consts/(.*)$",
    "^@constants/(.*)$",
    "^@utils/(.*)$",
    "^@assets/(.*)$",
    "^@tests/(.*)$",
    "^@mocks/(.*)$",
    "^[./]"
  ],
  "bracketSpacing": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
