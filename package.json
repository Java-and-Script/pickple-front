{
  "name": "pickple-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "env-cmd -f .env.development vite",
    "dev:prod": "env-cmd -f .env.production vite",
    "test": "env-cmd -f .env.development npx vitest --ui",
    "test:prod": "env-cmd -f .env.production npx vitest --ui",
    "preview": "env-cmd -f .env.production npm run build && vite preview",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:eslint": "eslint --ext ts,tsx \"src/**/*.{ts,tsx}\"",
    "lint:prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint:tsc": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^5.0.5",
    "@types/daum-postcode": "^2.0.2",
    "@types/sockjs-client": "^1.5.4",
    "@types/stompjs": "^2.3.8",
    "axios": "^1.5.1",
    "env-cmd": "^10.1.0",
    "event-source-polyfill": "^1.0.31",
    "react": "^18.2.0",
    "react-calendar": "^4.6.1",
    "react-dom": "^18.2.0",
    "react-draggable-bottom-sheet": "^1.14.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.47.0",
    "react-kakao-maps-sdk": "^1.1.24",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.17.0",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/event-source-polyfill": "^1.0.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "msw": "^2.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vercel": "^32.5.0",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^4.1.0",
    "vitest": "^0.34.6"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
